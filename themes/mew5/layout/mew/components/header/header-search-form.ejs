<div class="mew___components_header_header-search-form">
  <input class="search-form-input" id="header-post-search-form" type="text">

  <div class="search-results">
    <ul>
      <% site.posts.sort('title', 1).each(function(post){ %>
        <li class="header-post-search-item">
          <a href="<%= post.path %>">
            <p class="title"><%= post.title %></p>
            <div class="post-content hidden">
              <%- post.content %>">
            </div>
          </a>
        </li>
      <% }) %>
    </ul>
  </div>
</div>

