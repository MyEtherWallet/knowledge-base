<header>
  <div class="header-wrap page-container">
    <div class="top-block">
      <div class="left">
        <a href="<%- url_for('') %>"><%- image_tag('images/mew-logo-white.png') %></a>
        <p><%= _p('help_center') %></p>
      </div>
      <div class="right">
        <ul>
          <% for (var i in theme.menu){ %>
            <li><a class="main-nav-link" href="<%- url_for(theme.menu[i]) %>"><%= i %></a></li>
          <% } %>
        </ul>
      </div>
    </div>
    <div class="search-block header-search-block">
      <div class="search-title"><%= _p('tag_line') %></div>
      <div class="search-form-container">
        <%- partial('mew/components/header/header-search-form', null) %>
      </div>
      <p class="search-bottom-text">
        <%= _p('misc.top_searches') %>
        <a href="<%- url_for('posts/MEWconnect/MEWconnect_101_Create/') %>"><%= _p('index.mewconnect') %></a> /
        <a href="<%- url_for('posts/Swap/Buy_ETH_via_Simplex/') %>"><%= _p('index.swap') %></a> /
        <a href="<%- url_for('posts/Transactions/How_to_Send_a_Transaction/') %>"><%= _p('misc.send_eth') %></a> /
        <a href="<%- url_for('posts/Security-And-Privacy/Ensuring_MEW_Doesnt_Give_People_Same_Address/') %>"><%= _p('index.security') %></a> /
        <a href="<%- url_for('posts/Getting_Started/MyEtherWallet_An_Introduction/') %>"><%= _p('misc.blockchain') %></a> /
        <a href="<%- url_for('posts/Getting_Started/How_To_Create_A_Wallet/') %>"><%= _p('misc.create_wallet') %></a>
      </p>
    </div>
  </div>
</header>