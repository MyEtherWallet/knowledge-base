
<div class="mew___components_bottom-helpers">
  <div class="block-item">
    <div class="icon-image">
      <%- image_tag('images/mew-icons/customer-support.svg') %>
    </div>
    <div class="content">
      <div class="title"><%= _p('help.customer_support') %></div>
      <p><%= _p('help.cantFind') %></p>
      <div class="email-us">
        <a href="mailto:support@myetherwallet.com" target="_blank">
          <%= _p('help.email_us') %>
          <span>&#x2192;</span>
        </a>
      </div>
    </div>
  </div>
  <div class="block-item">
    <div class="icon-image">
      <%- image_tag('images/mew-icons/community.svg') %>
    </div>
    <div class="content">
      <div class="title"><%= _p('help.community') %></div>
      <p><%= _p('help.community_join') %></p>
      <div class="community-links">
        <a href="https://www.reddit.com/r/MyEtherWallet/" target="_blank">
          <%- image_tag('images/mew-icons/reddit.svg') %>
        <a href="https://github.com/MyEtherWallet" target="_blank">
          <%- image_tag('images/mew-icons/github.svg') %></a>
      </div>
    </div>
  </div>
</div>
<!-- .bottom-helpers -->
