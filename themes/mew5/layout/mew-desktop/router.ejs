

<% if (page.source == page.lang + '/categories/index.md' || page.source == page.lang + '/index.md' || !page.source) { %>
  <%- partial(theme.theme_desktop + '/components/header/header-search', null) %>
  <%- partial(theme.theme_desktop + '/categories/category-categories', null) %>

<% } else if (page.source == page.lang + '/faqs/index.md') { %>
  <%- partial(theme.theme_desktop + '/components/header/header-search', null) %>
  <%- partial(theme.theme_desktop + '/categories/category-faqs', null) %>

<% } else if (page.source == page.lang + '/videos/index.md') { %>
  <%- partial(theme.theme_desktop + '/components/header/header-search', null) %>
  <%- partial(theme.theme_desktop + '/categories/category-videos', null) %>

<% } else { %>
  <%- partial(theme.theme_desktop + '/components/header/header-page-title', null) %>
  <%- body %>
<% } %>

